# Bot configuration
BOT_TOKEN=your_telegram_bot_token_here

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/realestate

# Redis for sessions
REDIS_URL=redis://localhost:6379

# Mini App URL
MINI_APP_URL=https://your-app.vercel.app

# API URL
API_URL=http://localhost:3001

# OpenRouter settings for LLM (required for AI features)
OPENROUTER_API_KEY=sk-or-v1-your_openrouter_api_key

# LLM Preset (optional, default: balanced)
# Options: premium, balanced, economy, russian
LLM_PRESET=balanced

# App URL for OpenRouter headers
APP_URL=https://your-bot-domain.com

# Partner API settings
# PIK (крупный застройщик)
PIK_PARTNER_ID=your_pik_partner_id
PIK_API_KEY=your_pik_api_key
PIK_SECRET_KEY=your_pik_secret_key

# DomClick (Сбербанк)
DOMCLICK_PARTNER_ID=your_domclick_partner_id
DOMCLICK_API_KEY=your_domclick_api_key
DOMCLICK_SECRET_KEY=your_domclick_secret_key

# All-in-One провайдеры (база объектов + оплата за лиды)
# Яндекс.Недвижимость
YANDEX_REALTY_PARTNER_ID=your_yandex_partner_id
YANDEX_REALTY_API_KEY=your_yandex_api_key
YANDEX_REALTY_SECRET_KEY=your_yandex_secret_key
YANDEX_LEAD_PRICE_BASE=1500
YANDEX_LEAD_PRICE_QUALIFIED=3000
YANDEX_LEAD_PRICE_EXCLUSIVE=5000

# ЦИАН Партнер
CIAN_PARTNER_ID=your_cian_partner_id
CIAN_PARTNER_API_KEY=your_cian_api_key
CIAN_PARTNER_SECRET_KEY=your_cian_secret_key
CIAN_LEAD_PRICE_BASE=2000
CIAN_LEAD_PRICE_QUALIFIED=4000
CIAN_LEAD_PRICE_EXCLUSIVE=7000

# Admin IDs для команды /leads (через запятую)
ADMIN_IDS=123456789,987654321

# Environment
NODE_ENV=development
LOG_LEVEL=info