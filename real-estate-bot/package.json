{
  "name": "real-estate-bot",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "turbo run test",
    "db:migrate": "turbo run db:migrate",
    "db:push": "turbo run db:push",
    "type-check": "turbo run build --filter=...^#typecheck"
  },
  "packageManager": "npm@10.8.1",
  "devDependencies": {
    "@types/node": "^20.10.5",
    "prettier": "^3.1.1",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "@types/jest": "^29.5.12"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
