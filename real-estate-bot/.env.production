# Production Environment Variables
NODE_ENV=production

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/realestate_prod

# Redis
REDIS_URL=redis://localhost:6379

# Telegram
BOT_TOKEN=your_production_bot_token
BOT_USERNAME=YourProductionBot

# URLs
API_URL=https://api.realestate-bot.com
MINI_APP_URL=https://app.realestate-bot.com
APP_URL=https://realestate-bot.com

# Security
JWT_SECRET=your_very_secure_jwt_secret_key
TELEGRAM_BOT_SECRET=your_telegram_webhook_secret

# OpenRouter settings for LLM
OPENROUTER_API_KEY=sk-or-v1-your_production_key
LLM_PRESET=balanced

# Partner API settings
# Яндекс.Недвижимость
YANDEX_REALTY_PARTNER_ID=your_production_id
YANDEX_REALTY_API_KEY=your_production_key
YANDEX_REALTY_SECRET_KEY=your_production_secret

# ЦИАН Партнер
CIAN_PARTNER_ID=your_production_id
CIAN_PARTNER_API_KEY=your_production_key
CIAN_PARTNER_SECRET_KEY=your_production_secret

# Admin settings
ADMIN_IDS=your_telegram_id

# Monitoring
SENTRY_DSN=https://xxx@sentry.io/xxx
LOG_LEVEL=info

# Analytics
GA_TRACKING_ID=UA-XXXXXXXXX-X
YANDEX_METRIKA_ID=XXXXXXXX

# Email notifications (optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=notifications@realestate-bot.com
SMTP_PASS=your_smtp_password
ADMIN_EMAIL=admin@realestate-bot.com

# Backup settings
BACKUP_ENABLED=true
BACKUP_S3_BUCKET=realestate-bot-backups
AWS_ACCESS_KEY_ID=your_aws_key
AWS_SECRET_ACCESS_KEY=your_aws_secret
AWS_REGION=eu-central-1